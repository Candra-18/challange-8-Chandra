{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins":["jest"],
    "rules": {
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "class-methods-use-this": ["error", { "enforceForClassFields": false }],
        "no-undef":"off",
        "no-sequences":"off"
        

    }
}
